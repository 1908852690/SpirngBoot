
#代表springboot1数据源
spring.datasource.springboot1.driverClassName=com.mysql.jdbc.Driver
spring.datasource.springboot1.url=jdbc:mysql://localhost:3306/springboot01
spring.datasource.springboot1.username=root
spring.datasource.springboot1.password=


#代表springboot2数据源
spring.datasource.springboot2.driverClassName=com.mysql.jdbc.Driver
spring.datasource.springboot2.url=jdbc:mysql://localhost:3306/springboot01
spring.datasource.springboot2.username=root
spring.datasource.springboot2.password=